{
    "name": "product_management",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "tsx src/main/server.ts",
        "build": "tsc",
        "start": "node dist/src/main/server.js",
        "typeorm": "typeorm-ts-node-esm",
        "migration:create": "npm run typeorm -- migration:create",
        "migration:generate": "npm run typeorm -- migration:generate -d ormconfig.ts",
        "migration:run": "npm run typeorm -- migration:run -d ormconfig.ts",
        "migration:revert": "npm run typeorm -- migration:revert -d ormconfig.ts",
        "migration:show": "npm run typeorm -- migration:show -d ormconfig.ts",
        "test": "jest"
    },
    "dependencies": {
        "amqplib": "^0.10.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.1.13",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.20",
        "uuid": "^11.0.5"
    },
    "devDependencies": {
        "@types/amqplib": "^0.10.6",
        "@types/express": "^5.0.6",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.8",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.13.1",
        "@types/pg": "^8.11.11",
        "@types/supertest": "^6.0.3",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.7",
        "@types/uuid": "^10.0.0",
        "jest": "^30.2.0",
        "supertest": "^7.2.2",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3"
    }
}